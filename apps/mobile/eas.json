{
	"cli": {
		"version": ">= 18.0.1",
		"appVersionSource": "remote"
	},
	"build": {
		"development": {
			"developmentClient": true,
			"distribution": "internal",
			"channel": "development"
		},
		"preview": {
			"distribution": "internal",
			"android": {
				"buildType": "apk"
			},
			"channel": "preview",
			"env": {
				"EXPO_USE_METRO_WORKSPACE_ROOT": "1"
			}
		},
		"production": {
			"autoIncrement": true,
			"channel": "production"
		}
	},
	"submit": {
		"production": {}
	}
}
